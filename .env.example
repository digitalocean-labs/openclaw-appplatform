# Tailscale
TS_AUTHKEY=tskey-auth-xxx
TS_HOSTNAME=moltbot
TS_SOCKET=/tmp/tailscaled.sock
TS_ENABLE_HEALTH_CHECK=true
TS_LOCAL_ADDR_PORT=[::]:9002

# S3/Spaces for backups (Litestream and Restic)
SPACES_ENDPOINT=atl1.digitaloceanspaces.com
SPACES_BUCKET=your-bucket-name
LITESTREAM_ACCESS_KEY_ID=your-access-key
LITESTREAM_SECRET_ACCESS_KEY=your-secret-key

# Restic encryption password
RESTIC_PASSWORD=your-secure-password

# moltbot
MOLTBOT_STATE_DIR=/data/.moltbot
MOLTBOT_WORKSPACE_DIR=/data/workspace

# Optional
SETUP_PASSWORD=your-setup-password

# SSH
ENABLE_SSH=true
GITHUB_USERNAME=your-github-username